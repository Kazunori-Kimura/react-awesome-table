{
    "name": "@kazunori-kimura/react-awesome-table",
    "version": "0.6.0",
    "description": "JSON配列をいい感じにテーブル表示する React Component",
    "main": "build/index.js",
    "module": "build/index.es.js",
    "types": "types/index.d.ts",
    "files": [
        "package.json",
        "README.md",
        "LICENSE",
        "types",
        "build"
    ],
    "scripts": {
        "prebuild": "rimraf build types",
        "build:rollup": "rollup -c",
        "build:ts": "tsc -P tsconfig.types.json --emitDeclarationOnly",
        "build": "run-p -l -c --aggregate-output build:*",
        "fix": "run-s fix:prettier fix:eslint",
        "fix:eslint": "npm run lint:eslint -- --fix",
        "fix:prettier": "npm run lint:prettier -- --write",
        "lint": "run-p -l -c --aggregate-output lint:*",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "test": "jest",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o docs --docs",
        "prepublishOnly": "npm run build"
    },
    "keywords": [],
    "author": "kazunori-kimura",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@rollup/plugin-commonjs": "^18.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@material-ui/core": "^4.11.4",
        "@storybook/addon-actions": "^6.3.7",
        "@storybook/addon-essentials": "^6.3.7",
        "@storybook/addon-links": "^6.3.7",
        "@storybook/react": "^6.3.7",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@types/classnames": "^2.3.0",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "babel-loader": "^8.2.2",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.7.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.47.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-typescript2": "^0.30.0",
        "ts-jest": "^26.5.6",
        "typescript": "^4.2.4"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "@material-ui/core": "^4.11.4"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "react-app",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "no-use-before-define": "off",
            "@typescript-eslint/no-use-before-define": [
                "error"
            ]
        }
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "hotkeys-js": "^3.8.3"
    },
    "directories": {
        "doc": "docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kazunori-Kimura/react-awesome-table.git"
    },
    "bugs": {
        "url": "https://github.com/Kazunori-Kimura/react-awesome-table/issues"
    },
    "homepage": "https://github.com/Kazunori-Kimura/react-awesome-table#readme"
}
